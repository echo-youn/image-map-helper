{
  "name": "Image Map Helper",
  "description": "Chrome extension that helps image maps in HTML. HTML의 이미지맵을 도와주는 크롬 확장프로그램입니다.",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "Image Map Helper"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}